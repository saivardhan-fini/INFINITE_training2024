@model List<Assessment_1.Customer>

@{
    ViewBag.Title = "AllCustomersInGermany";
}

<h2>AllCustomersInGermany</h2>

<table class="table">
    <tr>
        <th>Customer ID</th>
        <th>Company Name</th>
        <th>Contact Name</th>
        <th>Contact Title</th>
        <th>Address</th>
    </tr>
    @foreach(var customer in Model)
            {
              <tr>
                  <td>@customer.CustomerID</td>
                  <td>@customer.CompanyName</td>
                  <td>@customer.ContactName</td>
                  <td>@customer.ContactTitle</td>
                  <td>@customer.Address</td>
              </tr>
            }
</table>



